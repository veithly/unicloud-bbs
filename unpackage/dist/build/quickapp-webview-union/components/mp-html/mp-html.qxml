<view class="{{[(selectable?'_select ':'')+'_root']}}" style="{{((editable?'position:relative;min-height:200px;':'')+containerStyle)}}" id="_root" data-event-opts="{{[['tap',[['_containTap',['$event']]]]]}}" bindtap="__e"><block qa:if="{{!nodes[0]}}"><slot></slot></block><block qa:else><node vue-id="ecbc38c8-1" childs="{{nodes}}" opts="{{[lazyLoad,loadingImg,errorImg,showImgMenu,editable,placeholder,'nodes']}}" name="span" bind:__l="__l"></node></block><block qa:if="{{tooltip}}"><view class="_tooltip_contain" style="{{('top:'+tooltip.top+'px')}}"><view class="_tooltip"><block qa:for="{{tooltip.items}}" qa:for-item="item" qa:for-index="index" qa:key="index"><view class="_tooltip_item" data-i="{{index}}" data-event-opts="{{[['tap',[['_tooltipTap',['$event']]]]]}}" bindtap="__e">{{item}}</view></block></view></view></block><block qa:if="{{slider}}"><view class="_slider" style="{{('top:'+slider.top+'px')}}"><slider style="padding:3px;" value="{{slider.value}}" min="{{slider.min}}" max="{{slider.max}}" handle-size="14" block-size="14" show-value="{{true}}" activeColor="white" data-event-opts="{{[['changing',[['_sliderChanging',['$event']]]],['change',[['_sliderChange',['$event']]]]]}}" bindchanging="__e" bindchange="__e"></slider></view></block></view>